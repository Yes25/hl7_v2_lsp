{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "hl7v2",
      "camelcase": "Hl7v2",
      "title": "Hl7v2",
      "scope": "source.hl7v2",
      "file-types": [
        "hl7",
        "txt"
      ],
      "injection-regex": "^hl7v2$",
      "class-name": "TreeSitterHl7v2"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A Parser for HL7 v2 Messages",
    "authors": [
      {
        "name": "Jesse Kruse",
        "email": "jesse.kruse@outlook.de"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-hl7v2"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}